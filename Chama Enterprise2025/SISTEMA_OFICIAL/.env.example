# Configuração do Sistema Rota Verde

# Ambiente: development | production
NODE_ENV=development

# Servidor
PORT=5000

# API URL (necessária para o Frontend se comunicar com o Backend)
VITE_API_URL=http://localhost:5000

# Banco de Dados
DATABASE_URL=postgres://user:password@localhost:5432/rota_verde

# Segurança
JWT_SECRET=sua_chave_secreta_super_segura_aqui

# Frontend URL (CORS)
FRONTEND_URL=http://localhost:5173
